from djitellopy import tello
from time import sleep

me = tello.Tello()
me.connect()

#launch

me.takeoff(120)
sleep(.5)

me.move_forward(550)
sleep(.5)
me.flip_left()
sleep(.5)
me.rotate_counter_clockwise(90)
sleep(.5)
me.move_forward(50)
sleep(.5)
me.rotate_counter_clockwise(90)
sleep(.5)
me.move_forward(600)
sleep(.5)
me.rotate_counter_clockwise(90)
sleep(.5)
me.move_forward(250)
sleep(.5)

me.land()
